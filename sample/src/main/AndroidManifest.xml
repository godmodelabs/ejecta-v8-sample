<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ag.boersego.bgjs.sample" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="ag.boersego.bgjs.sample.DemoListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="ag.boersego.bgjs.sample.DemoDetailActivity"
            android:label="@string/title_demo_detail"
            android:parentActivityName="ag.boersego.bgjs.sample.DemoListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ag.boersego.bgjs.sample.DemoListActivity" />
        </activity>
    </application>

</manifest>
